#include<stdio.h>
int main(){
    int arr[3][3];
    for (int i=0; i<3; i++){
        for(int j=0; j<3; j++){
        printf("the %d th row and %d th column element is:",i,j);
        scanf("%d",arr[i][j]);
    }
    }
    int sum =0;
    int temp;
    for(int i=0;i<3;i++){
        for (int j=0; j<3;j++){
            sum+=arr[i][j];
        }    
        if (sum>0){
            temp=sum;
        }
        }
    return 0;
}